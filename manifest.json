{
  "manifest_version": 2,

  "name": "Search Auto-Focus",
  "description": "Auto-focuses search field on websites when you start to type",
  "version": "2.0",

  "icons": {
    "48": "logo-48.png",
    "128": "logo-128.png"
  },

  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
  }],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
